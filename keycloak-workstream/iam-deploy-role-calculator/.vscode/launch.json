      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug CDK",
      "skipFiles": ["<node_internals>/**"],
      "runtimeArgs": ["-r", "./node_modules/ts-node/register/transpile-only"],
      "args": ["./bin/main.ts"],
      "envFile": "${workspaceFolder}/.env",
      "env": {      
        "AWS_DEFAULT_REGION": "us-west-1"
      }
    }
  ]
