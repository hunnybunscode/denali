      "script": "${file}",
      "cwd": "${workspaceFolder}",
      "args": [
        "-Enhance",
        "-TemplateFile",
        // "/Users/jktruong/workspace/engagements/c3e/bootstrap/jktruong+team-sentinel-taser-SharedServices-Admin.us-east-1.cdk.out/SapDomainServicesStack.template.json"
